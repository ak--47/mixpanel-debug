{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "mixpanel-debug",
  "description": "a browser extension to debug mixpanel web tracking",
  "version": "1.0.0",
  "icons": {
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "96": "images/96x96.png",
    "128": "images/128x128.png"
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "permissions": ["storage", "webRequest"],
  "content_scripts": [
    {
      "css": [],
      "js": ["dist/toast.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "public/iron.min.js",
        "public/runtime.min.js",
        "public/node_modules.min.js",
        "public/bundle.min.css",
        "public/mixpanel.css",
        "public/injectToasts.js",
        "public/apercu-regular-pro.woff2"
      ],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "content_security_policy": {},
  "action": {
    "default_popup": "public/popup.html",
    "default_icon": {
      "32": "images/32x32.png",
      "48": "images/48x48.png",
      "96": "images/96x96.png",
      "128": "images/128x128.png"
    }
  },
  "options_ui": {
    "page": "public/settings.html",
    "open_in_tab": true
  }
}
